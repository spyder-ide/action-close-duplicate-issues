{
  "name": "actions-close-duplicate-issues",
  "version": "1.0.0",
  "description": "GitHub action for closing duplicate issues",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "pack": "ncc build lib/main.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spyder-ide/actions-close-duplicate-issues"
  },
  "keywords": [
    "actions",
    "container",
    "issues",
    "comment",
    "duplicate",
    "close",
    "message"
  ],
  "author": "Spyder Contributors",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/spyder-ide/actions-close-duplicate-issues/issues"
  },
  "homepage": "https://github.com/spyder-ide/actions-close-duplicate-issues#readme",
  "dependencies": {
    "@actions/core": "^1.0.0",
    "@actions/io": "^1.0.0",
    "@actions/exec": "^1.0.0",
    "@actions/github": "^1.0.0",
    "@actions/tool-cache": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^12.0.4",
    "prettier": "^1.17.1",
    "typescript": "^3.5.1",
    "husky": "^2.3.0"
  },
  "husky": {
    "skipCI": true,
    "hooks": {
      "pre-commit": "npm run build && npm run format && npm run pack"
    }
  }
}
